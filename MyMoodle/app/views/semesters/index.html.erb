<h1>Listing semesters</h1>

<table>
  <thead>
    <tr>
      <th>Semester start</th>
      <th>Semester end</th>
      <th></th>
      <th></th>
      <th></th>
    </tr>
  </thead>

  <tbody>
    <% @semesters.each do |semester| %>
      <tr>
        <td><%= semester.start %></td>
        <td><%= semester.end %></td>
        <td><%= link_to 'Show', semester %></td>
        <td><%= link_to 'Edit', edit_semester_path(semester) %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>